{
  "name": "weekly-shopping",
  "private": true,
  "packageManager": "npm@10.9.0",
  "engines": {
    "npm": ">=8",
    "node": ">=18"
  },
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "format": "prettier -w \"**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "turbo": "^2.1.3",
    "typescript": "^5.6.2"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}

